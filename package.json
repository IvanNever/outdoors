{
  "name": "outdoors",
  "version": "1.0.0",
  "description": "landing page for outdoors",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "start": "live-server watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "compress:css": "node-sass css/style.concat.css css/style.min.css --output-style compressed"
  },
  "repository": {
    "type": "git"
  },
  "author": "Ivan Neverovsky",
  "bugs": {
    "url": "https://github.com/IvanNever/outdoors/issues"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss-cli": "^7.1.1"
  },
  "dependencies": {}
}
